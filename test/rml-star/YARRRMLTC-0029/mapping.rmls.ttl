<#elementaryTM1_0> a rml:NonAssertedTriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "data.csv";
		rml:referenceFormulation ql:CSV
	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://example/$(c1)";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant ex:p1;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://example/$(c2)";
		];
	].

