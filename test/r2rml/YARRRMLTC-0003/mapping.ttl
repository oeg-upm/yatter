@base <http://example.com/ns#>.


<student_0> a rr:TriplesMap;

	rr:logicalTable [
		a rr:LogicalTable;
		rr:sqlQuery "Select ('Student' || \"ID\" ) AS StudentId , \"ID\" , \"Name\" from \"Student\" ";
		rr:sqlVersion rr:SQL2008
	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://example.com/{ID}/{Name}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant foaf:name;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:column "Name";
		];
	].


